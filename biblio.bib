@article{Clodic2005,
	Abstract = {The presence of humans in its environment and the necessity to interact
	with them raise new challenges to the robot. Indeed, they require
	explicit reasoning on the human environment and on the robot capacities
	to achieve its tasks in a collaborative way with a human partner.
	This paper focuses on architectural aspects and more particularly
	on the organization of the robot decisional abilities for interacting
	with people. Indeed, our objective is to develop a management of
	human interaction that would be an integral part of a general robot
	control architecture. This should hopefully allow to come up with
	a principled way to deal with human-robot interaction for robot task
	achievement in presence of humans or in synergy with humans. Such
	an architecture should be the framework that would allow the robot
	to produce behaviors to accomplish its tasks but also produce behaviors
	that support its engagement vis-a-vis its human partner and interpret
	similar behaviors from him. We also intend to use the proposed approach
	as a framework in which we would develop and experiment various task
	planners and interaction schemes. Indeed, the robot should be able,
	for instance, to devise plans that allow it to execute its actions
	and to place itself to be seen by or to observe humans, according
	to the task.},
	Author = {Clodic, A. and Montreuil, V. and Alami, R. and Chatila, R.},
	Doi = {10.1109/ROMAN.2005.1513836},
	File = {:Clodic2005.pdf:PDF},
	Imprime = {oui},
	Interet = {5},
	Journal = {Robot and Human Interactive Communication, 2005. ROMAN 2005. IEEE International Workshop on},
	Keywords = {decision making, man-machine systems, mobile robots, task analysis autonomous robots, human robot interaction, robot control architecture, robot decisional abilities, robot task},
	Lu = {oui},
	Month = {Aug.},
	Notes = {- Approche architecturale interessante - Liste ce qu'on attend d'un robot interagissant avec des humains - Notion d'agents proxy (IAAs) pour repr{\'e}senter les hommes dans le mod{\`e}le du robot, avec un IAA manger (suivi dynamique des humains) - Id{\'e}e de "Task delegate" -> esp{\`e}ce de "t{\^a}che intelligente" qui observe et contr{\^o}le son d{\'e}roulement REF: Joint Intention theory},
	Owner = {slemaign},
	Pages = {543-548},
	Relevance = {3},
	Timestamp = {2008.09.19},
	Title = {A decisional framework for autonomous robots interacting with humans},
	Year = {2005},
	Bdsk-Url-1 = {http://dx.doi.org/10.1109/ROMAN.2005.1513836}}

@article{Coradeschi2003,
	Abstract = {Anchoring is the problem of connecting, inside an artificial system,
	symbols and sensor data that refer to the same physical objects in
	the external world. This problem needs to be solved in any robotic
	system that incorporates a symbolic component. However, it is only
	recently that the anchoring problem has started to be addressed as
	a problem per se, and a few general solutions have begun to appear
	in the literature. This paper introduces the special issue on perceptual
	anchoring of the Robotics and Autonomous Systems journal. Our goal
	is to provide a general overview of the anchoring problem, and highlight
	some of its subtle points.},
	Author = {Coradeschi, S. and Saffiotti, A.},
	Doi = {doi:10.1016/S0921-8890(03)00021-6},
	File = {:Coradeschi2003.pdf:PDF},
	Interet = {5},
	Journal = {Robotics and Autonomous Systems},
	Keywords = {grounding},
	Lu = {lu},
	Notes = {Introduction to the Robotics and Automation System special issue on perceptual anchoring. - Good introduction to the question. - They present their own architecture - Summary of other contributions in the special issue They define the concept of anchoring Some random notes: * Anchoring differs from generic symbol grounding by the fact that it deals with the grounding of individual physical objects, excluding the grounding of their properties * Anchoring also implies a form of temporal maintenance of the grounding.},
	Number = {2-3},
	Owner = {skadge},
	Pages = {85--96},
	Publisher = {Elsevier},
	Relevance = {5},
	Timestamp = {2009.11.24},
	Title = {An introduction to the anchoring problem},
